{
  "name": "@lichthagel/eslint-config",
  "version": "0.0.0",
  "description": "my personal eslint-config to extend on",
  "keywords": [
    "eslint",
    "eslintconfig",
    "styleguide"
  ],
  "author": "Lichthagel",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "pnpm run typegen && tsup --format esm,cjs --clean --dts",
    "typegen": "tsx scripts/typegen.ts",
    "prepack": "pnpm run build",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "eslint .",
    "inspect": "pnpx @eslint/config-inspector"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@9.12.1",
  "dependencies": {
    "@eslint/js": "^9.11.1",
    "@stylistic/eslint-plugin": "^2.9.0",
    "eslint-plugin-perfectionist": "^3.7.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.9.0",
    "typescript-eslint": "^8.7.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.4",
    "eslint": "^9.11.1",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-svelte": "^2.44.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-typegen": "^0.3.2",
    "svelte": "^4.2.19",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "eslint": ">=8.40.0",
    "eslint-plugin-n": "^17.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0 || ^5.0.0",
    "eslint-plugin-svelte": "^2.39.0",
    "eslint-plugin-tailwindcss": "^3.17.4"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-n": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    }
  }
}