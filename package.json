{
  "name": "eslint-config-lichthagel",
  "version": "2.0.0",
  "description": "my personal eslint-config to extend on",
  "keywords": [
    "eslint",
    "eslintconfig",
    "styleguide"
  ],
  "author": "Lichthagel",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run typegen && tsup --format esm,cjs --clean --dts",
    "typegen": "tsx scripts/typegen.ts",
    "prepack": "npm run build",
    "lint": "eslint ."
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@9.1.2",
  "dependencies": {
    "@eslint/js": "^9.1.1",
    "@stylistic/eslint-plugin": "^2.0.0",
    "eslint-flat-config-utils": "^0.2.3",
    "eslint-plugin-unicorn": "^53.0.0",
    "globals": "^15.0.0"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.12.7",
    "eslint": "^9.1.0",
    "eslint-plugin-n": "^17.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-typegen": "^0.2.3",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.0"
  },
  "peerDependencies": {
    "eslint": ">=8.40.0",
    "eslint-plugin-n": "^17.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript-eslint": "^7.7.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-n": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  }
}
