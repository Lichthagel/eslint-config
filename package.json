{
  "name": "@lichthagel/eslint-config",
  "version": "1.0.31",
  "description": "Licht's ESLint config",
  "keywords": [
    "eslint",
    "eslintconfig",
    "styleguide"
  ],
  "author": "Lichthagel <hey@lichthagel.de>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lichthagel/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/Lichthagel/eslint-config/issues"
  },
  "homepage": "https://github.com/Lichthagel/eslint-config",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "pnpm run typegen && tsdown",
    "typegen": "tsx scripts/typegen.ts",
    "prepack": "pnpm run build",
    "watch": "tsdown --format esm,cjs --watch",
    "test": "vitest",
    "lint": "eslint .",
    "inspect": "pnpx @eslint/config-inspector",
    "prepare": "husky"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@eslint/js": "^9.11.1",
    "@stylistic/eslint-plugin": "^5.0.0",
    "eslint-plugin-perfectionist": "^4.0.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.0.0",
    "typescript-eslint": "^8.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "^24.0.0",
    "eslint": "^9.11.1",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-solid": "^0.14.3",
    "eslint-plugin-svelte": "^3.0.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-typegen": "^2.0.0",
    "husky": "^9.1.6",
    "jiti": "^2.4.2",
    "lint-staged": "^16.0.0",
    "svelte": "^5.12.0",
    "tsdown": "^0.16.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^4.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8.40.0",
    "eslint-plugin-n": "^17.2.1",
    "eslint-plugin-solid": "^0.14.3",
    "eslint-plugin-svelte": "^2.39.0 || ^3.0.0",
    "eslint-plugin-tailwindcss": "^3.17.4"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-n": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    }
  },
  "pnpm": {
    "overrides": {
      "@types/eslint": "npm:eslint@^9.11.1"
    }
  }
}
